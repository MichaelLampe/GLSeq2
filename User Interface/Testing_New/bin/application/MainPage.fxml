<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="-Infinity" prefHeight="786.0" prefWidth="873.0" stylesheets="@MainPage.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainPageController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TabPane fx:id="tab_check" minHeight="700.0" prefHeight="700.0" prefWidth="855.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Data Locations">
            <content>
              <AnchorPane fx:id="locations_pane" minHeight="700.0" minWidth="0.0" prefHeight="700.0" prefWidth="873.0" styleClass="tab_pane">
                     <children>
                        <TextArea fx:id="source_directory" layoutX="13.0" layoutY="94.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the directory of your data files? (Example: /home/data/myfiles/&quot;)" wrapText="true" />
                        <TextArea fx:id="destination_directory" layoutX="13.0" layoutY="169.0" prefHeight="32.0" prefWidth="848.0" promptText="In what directory should we put the results? (Example: /home/runs/)" wrapText="true" />
                        <TextArea fx:id="countable_sam_directory" layoutX="13.0" layoutY="334.0" prefHeight="32.0" prefWidth="848.0" promptText="In what directory are your countable.sam files?" wrapText="true" />
                        <Label layoutX="14.0" layoutY="30.0" text="Create a new run" textFill="WHITE">
                           <font>
                              <Font size="37.0" />
                           </font>
                        </Label>
                        <Label layoutX="14.0" layoutY="260.0" text="Analyze a previous run" textFill="WHITE">
                           <font>
                              <Font size="37.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="previous_run_directory" layoutX="13.0" layoutY="383.0" prefHeight="32.0" prefWidth="848.0" promptText="What was the directory of your previous run?" wrapText="true" />
                        <TextArea fx:id="previous_run_name" layoutX="13.0" layoutY="433.0" prefHeight="32.0" prefWidth="848.0" promptText="And what did you name that run?" wrapText="true" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Data Characteristics">
               <content>
                  <AnchorPane minHeight="700.0" prefHeight="200.0" prefWidth="200.0" styleClass="tab_pane">
                     <children>
                        <TextArea fx:id="strain" layoutX="13.0" layoutY="38.0" prefHeight="32.0" prefWidth="848.0" promptText="What is the strain of your data?" />
                        <ComboBox fx:id="strandedness" layoutX="14.0" layoutY="140.0" prefHeight="25.0" prefWidth="363.0" promptText="What is the strandedness of your library?" />
                        <ComboBox fx:id="sequencing_platform" layoutX="14.0" layoutY="340.0" prefHeight="25.0" prefWidth="363.0" promptText="What sequencing platform did you use to gather this data?" />
                        <ComboBox fx:id="quality_score" layoutX="14.0" layoutY="240.0" prefHeight="25.0" prefWidth="363.0" promptText="What is the quality score format of your library?" />
                        <ComboBox fx:id="zipped" layoutX="456.0" layoutY="140.0" prefHeight="25.0" prefWidth="363.0" promptText="Is your data zipped or unzipped format?" />
                        <ComboBox fx:id="endedness" layoutX="456.0" layoutY="240.0" prefHeight="25.0" prefWidth="363.0" promptText="Is it paired ended or single ended?" />
                        <ComboBox fx:id="presplit" layoutX="456.0" layoutY="340.0" prefHeight="25.0" prefWidth="363.0" promptText="Is it presplit or unsplit?" />
                        <Label layoutX="14.0" layoutY="100.0" text="Library Strandedness" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="456.0" layoutY="100.0" text="Zipped or Unzipped" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="456.0" layoutY="200.0" text="Paired or Single Ended" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="14.0" layoutY="200.0" text="Quality Score Format" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="14.0" layoutY="300.0" text="Sequencing Platform" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="456.0" layoutY="300.0" text="Presplit or Unsplit" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
            <Tab text="Processing Algorithms">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="tab_pane">
                     <children>
                        <Label layoutX="425.0" layoutY="30.0" text="Select whichever counting methods you'd like to use" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="reference_genome" layoutX="14.0" layoutY="300.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the name of your reference genome? (Example: &quot;Y22.3.A144&quot;)" />
                        <TextArea fx:id="reference_fasta" layoutX="13.0" layoutY="340.0" prefHeight="32.0" prefWidth="848.0" promptText="How about the name of your reference FASTA file? (Example: &quot;Y22-3.assembly.fasta&quot;)&quot;)" />
                        <TextArea fx:id="reference_gtf" layoutX="14.0" layoutY="380.0" prefHeight="32.0" prefWidth="848.0" promptText="What is the name of your reference genomic features file (GTF)? (Example: &quot;Y22-3.A144c.gtf&quot;)" />
                        <TextArea fx:id="feature_id_gtf" layoutX="13.0" layoutY="420.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the reference Feature ID for that GTF file? (Example: &quot;gene_id&quot;)" />
                        <Label fx:id="gtf_column_label" layoutX="14.0" layoutY="474.0" text="Your GTF file has 9 columns" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Slider fx:id="gtf_column_slider" layoutX="14.0" layoutY="519.0" majorTickUnit="1.0" max="20.0" minorTickCount="0" prefHeight="14.0" prefWidth="846.0" showTickMarks="true" value="9.0" />
                        <Slider fx:id="max_fragment_length_slider" layoutX="14.0" layoutY="599.0" majorTickUnit="100.0" max="5000.0" minorTickCount="0" prefHeight="14.0" prefWidth="846.0" showTickMarks="true" value="1000.0" />
                        <Label fx:id="max_fragment_length_label" layoutX="14.0" layoutY="554.0" text="The maximal fragment length is 1000" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label layoutX="13.0" layoutY="17.0" text="Alignment Algorithm" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <CheckBox layoutX="425.0" layoutY="65.0" mnemonicParsing="false" text="HTSeq" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <CheckBox layoutX="425.0" layoutY="115.0" mnemonicParsing="false" text="FeatureCounts" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <CheckBox layoutX="425.0" layoutY="165.0" mnemonicParsing="false" text="RSEM" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <CheckBox layoutX="425.0" layoutY="215.0" mnemonicParsing="false" text="Cufflinks" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <RadioButton fx:id="bwa_radio" layoutX="14.0" layoutY="50.0" mnemonicParsing="false" text="BWA" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="bowtie_radio" layoutX="14.0" layoutY="90.0" mnemonicParsing="false" text="Bowtie" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="bowtie2_radio" layoutX="14.0" layoutY="130.0" mnemonicParsing="false" text="Bowtie2" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="cushaw_radio" layoutX="181.0" layoutY="50.0" mnemonicParsing="false" text="Cushaw" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="cushaw_gpu_radio" layoutX="181.0" layoutY="90.0" mnemonicParsing="false" text="Cushaw-GPU" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="tophat_radio" layoutX="14.0" layoutY="170.0" mnemonicParsing="false" text="TopHat" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="rockhopper_radio" layoutX="181.0" layoutY="130.0" mnemonicParsing="false" text="Rockhopper" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </RadioButton>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Computation Settings">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="tab_pane">
                     <children>
                        <Slider fx:id="cores_slider" blockIncrement="1.0" layoutX="14.0" layoutY="68.0" majorTickUnit="1.0" max="48.0" min="1.0" minorTickCount="0" prefHeight="14.0" prefWidth="838.0" showTickMarks="true" />
                        <Label fx:id="cores_label" layoutX="14.0" layoutY="23.0" text="You are now using 1 core" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>s
                        </Label>
                        <Label fx:id="dataprep_streams_label" layoutX="14.0" layoutY="103.0" text="1 computational stream during data preparation" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Slider fx:id="dataprep_streams_slider" blockIncrement="1.0" layoutX="14.0" layoutY="148.0" majorTickUnit="1.0" max="10.0" min="1.0" minorTickCount="0" prefHeight="14.0" prefWidth="838.0" showTickMarks="true" />
                        <Slider fx:id="alignment_streams_slider" blockIncrement="1.0" layoutX="14.0" layoutY="228.0" majorTickUnit="1.0" max="10.0" min="1.0" minorTickCount="0" prefHeight="14.0" prefWidth="838.0" showTickMarks="true" />
                        <Label fx:id="alignment_streams_label" layoutX="14.0" layoutY="183.0" text="1 computational stream during alignment" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="max_buffer_label" layoutX="14.0" layoutY="263.0" text="Maximum auxiliary buffer when computing CIs is 4096" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Slider fx:id="max_buffer_slider" blockIncrement="1028.0" layoutX="14.0" layoutY="308.0" majorTickUnit="1028.0" max="12288.0" minorTickCount="0" prefHeight="14.0" prefWidth="838.0" showTickMarks="true" value="4096.0" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Trim Settings">
               <content>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="tab_pane">
                     <children>
                        <ComboBox fx:id="trim" layoutX="15.0" layoutY="60.0" prefHeight="25.0" prefWidth="261.0" promptText="Would you like to trim your raw reads?" />
                        <Slider fx:id="trim_head_slider" layoutX="15.0" layoutY="215.0" majorTickUnit="1.0" max="25.0" minorTickCount="0" prefHeight="14.0" prefWidth="838.0" showTickMarks="true" value="12.0" />
                        <Slider fx:id="min_read_length_slider" layoutX="15.0" layoutY="295.0" majorTickUnit="1.0" max="75.0" minorTickCount="0" prefWidth="838.0" showTickMarks="true" value="36.0" />
                        <Label fx:id="trim_head_label" layoutX="15.0" layoutY="170.0" text="We are currently trimming 12 bases from the head" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="min_read_length_label" layoutX="15.0" layoutY="250.0" text="Reads under 36 bases will be discarded" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="artificial_fasta" layoutX="14.0" layoutY="110.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the name of your FASTA file that contains your artificial sequences? (Example: &quot;JGI.clean2.fa&quot;)" />
                        <Label layoutX="15.0" layoutY="24.0" text="Trim Raw Reads" textFill="WHITE">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children></AnchorPane>
               </content>
            </Tab>
            <Tab text="Environment">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="tab_pane">
                     <children>
                        <TextArea fx:id="script_directory" layoutX="14.0" layoutY="10.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the directory that the GLSeq R scripts are located in?" />
                        <TextArea fx:id="trimmomatic_path" layoutX="14.0" layoutY="60.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the location of the Trimmomatic jar file?" />
                        <TextArea fx:id="fastqc_path" layoutX="14.0" layoutY="160.0" prefHeight="32.0" prefWidth="848.0" promptText="What is the path to the FastQC program?" />
                        <TextArea fx:id="picardTools_directory" layoutX="14.0" layoutY="110.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the directory that stores your PicardTools jars?" />
                        <TextArea fx:id="bwa_path" layoutX="14.0" layoutY="210.0" prefHeight="32.0" prefWidth="848.0" promptText="What is the path to your BWA aligner?" />
                        <TextArea fx:id="bam2wig_path" layoutX="14.0" layoutY="260.0" prefHeight="32.0" prefWidth="848.0" promptText="What is the path to the Bam2Wig shell script?" />
                        <TextArea fx:id="rockhopper_path" layoutX="14.0" layoutY="510.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the path to your Rockhopper aligner?" />
                        <TextArea fx:id="cushaw_gpu_path" layoutX="14.0" layoutY="410.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the path to your CUSHAW-GPU aligner?" />
                        <TextArea fx:id="tophat_path" layoutX="14.0" layoutY="460.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the path to your TopHat aligner?" />
                        <TextArea fx:id="cushaw_path" layoutX="14.0" layoutY="360.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the path to your CUSHAW aligner?" />
                        <TextArea fx:id="cushaw_index_path" layoutX="14.0" layoutY="310.0" prefHeight="32.0" prefWidth="848.0" promptText="What's the path to your CUSHAW indexer?" />
                        <TextArea fx:id="logs_path" layoutX="14.0" layoutY="560.0" prefHeight="32.0" prefWidth="848.0" promptText="Where should we put our log files?" />
                     </children></AnchorPane>
               </content>
            </Tab>
            <Tab text="Run">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" styleClass="tab_pane">
                     <children>
                        <Button fx:id="start_run" layoutX="14.0" layoutY="567.0" mnemonicParsing="false" prefHeight="95.0" prefWidth="846.0" text="Start Run">
                           <font>
                              <Font size="36.0" />
                           </font>
                        </Button>
                        <CheckBox fx:id="data_prep_check" layoutX="14.0" layoutY="132.0" mnemonicParsing="false" prefHeight="67.0" prefWidth="377.0" text="Data Preparation" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="alignment_check" layoutX="14.0" layoutY="211.0" mnemonicParsing="false" prefHeight="67.0" prefWidth="377.0" text="Alignment" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="counting_check" layoutX="14.0" layoutY="295.0" mnemonicParsing="false" prefHeight="67.0" prefWidth="377.0" text="Counting" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="collect_check" layoutX="14.0" layoutY="379.0" mnemonicParsing="false" prefHeight="67.0" prefWidth="377.0" text="Collect" textFill="WHITE">
                           <font>
                              <Font size="27.0" />
                           </font>
                        </CheckBox>
                        <Label layoutX="14.0" layoutY="33.0" prefHeight="27.0" prefWidth="838.0" text="Select which steps you'd like to execute and hit run!" textFill="WHITE">
                           <font>
                              <Font size="30.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="run_name" layoutX="13.0" layoutY="485.0" prefHeight="32.0" prefWidth="848.0" promptText="What do you want to call the run name?" />
                     </children></AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <Pane fx:id="footer_bar" prefHeight="61.0" prefWidth="68.0">
         <children>
            <ProgressBar fx:id="footer_progress" layoutX="14.0" layoutY="14.0" prefHeight="18.0" prefWidth="846.0" progress="0.24" />
         </children>
      </Pane>
   </children>
</VBox>
